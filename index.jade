    nav(class="navbar")
      div(class="row", id="navbar1")
        a(href="#", class="toggle", gumby-trigger="#nav1 > ul")
          i(class="icon-menu")
        h1(class="four columns logo")
          a(href="#")
            img(src="/img/gumby_mainlogo.png", gumby-retina)


    section(class="row")
      section(class="two columns")
        nav(gumby-fixed="top")
          h4
            a(href="#colors", class="skip" , gumby-update="", gumby-duration="600", gumby-goto="[data-target='colors']")
              |colors |fonts
          hr
          
          h4
            a(href="#layout", class="skip" , gumby-update="", gumby-duration="600", gumby-goto="[data-target='layout']")
